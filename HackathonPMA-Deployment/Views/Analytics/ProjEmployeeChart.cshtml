@{
    var xColumn = ViewBag.aCol;
    var yColumn = ViewBag.bCol;
    var myChart = new Chart(width: 600, height: 400, theme: ChartTheme.Green)
        .AddTitle("Project by Stakeholders Chart")
        .AddLegend("Number of Stakeholders per Project")
        .AddSeries(
            name: "Project-Stakeholders",
            @* xValue: new[] { "Peter", "Andrew", "Julie", "Mary", "Dave" },
                yValues: new[] { "2", "6", "4", "5", "3" }, *@
           xValue: xColumn,
           yValues: yColumn,
           xField: "Projects", yFields: "Stakeholders", chartType: "bar")
            .Write();
}