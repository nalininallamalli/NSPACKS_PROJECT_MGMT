@{
    var xColumn = ViewBag.xCol;
    var yColumn = ViewBag.yCol;
    var myChart = new Chart(width: 600, height: 400, theme: ChartTheme.Green)
        .AddTitle("Custom Chart")
        .AddLegend("Funds in Rupees Per instance")
        .AddSeries(
            name: "Project-Funds",
           @* xValue: new[] { "Peter", "Andrew", "Julie", "Mary", "Dave" },
            yValues: new[] { "2", "6", "4", "5", "3" }, *@
           xValue: xColumn,
           yValues: yColumn,
            xField: "Sub-Projects", yFields: "Funds", chartType: "pie")
            .Write();
}

