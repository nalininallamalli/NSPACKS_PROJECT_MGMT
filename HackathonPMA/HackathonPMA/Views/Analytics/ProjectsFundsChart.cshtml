@{
    var xColumn = ViewBag.xCol;
    var yColumn = ViewBag.yCol;

    foreach (string item in xColumn)
    {
       @Html.ActionLink(item, "ByProjectChart", "Analytics");
      //  item = "<a href="~/Views/Analytics/ByProjectChart.cshtml">item</a>";
    }

    var myChart = new Chart(width: 900, height: 600, themePath:"./theme.xml")
        .AddTitle("Project-Fund Chart")
        .AddLegend("Percentage Distribution")
        .AddSeries( 
            name: "Project-Funds",
            xValue: xColumn,
            yValues: yColumn,
            chartType: "pie")
            .Write();
}
