@{
    var xColumn = ViewBag.xCol;
    var yColumn = ViewBag.yCol;
    var myChart = new Chart(width: 600, height: 400, theme: ChartTheme.Green)
        .AddTitle("Project Distribution per City")
        .AddLegend("Number of Project Instances per City")
        .AddSeries(
            name: "Number of Project Instances",
         xValue: xColumn,
         yValues: yColumn,
         xField: "Cities", yFields: "Project Instances", chartType: "Column")
         .Write();
}