@{
    var xColumn = ViewBag.xCol;
    var yColumn = ViewBag.yCol;
    var zColumn = ViewBag.zCol;
    var myChart = new Chart(width: 600, height: 400, theme: ChartTheme.Green)
        .AddTitle("Inventory Distribution")
        .AddLegend("Allocated vs. Spent Inventories")
        .AddSeries(
            name: "Inventories",
        // xValue: new[] { "Peter", "Andrew", "Julie", "Mary", "Dave" },
        // yValues: new[] { "2", "6", "4", "5", "3" },
            xValue: xColumn,
            yValues: yColumn,
            xField: "Inventories", yFields: "Allocated Inventories", chartType: "line")
        .AddSeries(
           name: "Spent Inventories",
                yValues: zColumn,
                 chartType: "line"
           )
        .Write();
}

