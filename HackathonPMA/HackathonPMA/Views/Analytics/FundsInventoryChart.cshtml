@{
    var xColumn = ViewBag.xCol;
    var yColumn = ViewBag.yCol;
    var myChart = new Chart(width: 600, height: 400, theme: ChartTheme.Green)
        .AddTitle("Volunteer Distribution per City")
        .AddLegend("Number of Volunteers per City")
        .AddSeries(
            name: "Number of Volunteers",
         xValue: xColumn,
         yValues: yColumn,
         xField: "Cities", yFields: "Volunteers", chartType: "bar")
         .Write();
}

